name: " iRingo: 📺 TV app"
desc: |-
  全面自定义「TV app」功能
openUrl: "http://boxjs.com/#/app/iRingo.TV"
author: |-
  VirgilClyne[https://github.com/VirgilClyne]
homepage: "https://NSRingo.github.io/guide/apple-tv"
icon: "https://developer.apple.com/assets/elements/icons/apple-tv/apple-tv-128x128.png"
category: " iRingo"
date: "2024-12-27 08:11:56"
version: "3.4.3"

http:
  mitm:
    - "uts-api.itunes.apple.com"
    - "umc-tempo-api.apple.com"
  script:
    - match: https?:\/\/uts-api\.itunes\.apple\.com\/uts\/(v1|v2|v3)\/
      name: TV.request
      type: request
      require-body: true
    - match: https?:\/\/uts-api\.itunes\.apple\.com\/uts\/(v1|v2|v3)\/
      name: TV.response
      type: response
      require-body: true
    - match: ^https?:\/\/umc-tempo-api\.apple\.com\/(v1|v2|v3)\/
      name: TV.request
      type: request
      require-body: true
    - match: ^https?:\/\/play(-edge)?\.itunes\.apple\.com\/WebObjects\/MZPlay(Local)?\.woa\/hls(\/(subscription|workout))?\/playlist\.m3u8
      name: TV.request
      type: request
    - match: ^https?:\/\/play(-edge)?\.itunes\.apple\.com\/WebObjects\/MZPlay(Local)?\.woa\/wa\/(fpsRequest|checkInNonceRequest)
      name: TV.request
      type: request

script-providers:
  TV.request:
    url: https://github.com/NSRingo/TV/releases/download/v3.4.3/request.bundle.js
    interval: 86400
  TV.response:
    url: https://github.com/NSRingo/TV/releases/download/v3.4.3/response.bundle.js
    interval: 86400
